{
  "name": "safeline",
  "version": "1.0.0",
  "private": true,
  "prisma": {
    "schema": "./prisma/schema/"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npm run prisma:push && npm run prisma:deploy && npm run prisma:generate",
    "prisma:deploy": "prisma migrate deploy --schema=./prisma/schema/",
    "prisma:generate": "prisma generate --schema=./prisma/schema/",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull --schema=./prisma/schema/"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.2",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.0.1",
    "@material-tailwind/react": "^2.1.10",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.7.0",
    "@types/nodemailer": "^6.4.17",
    "bcrypt": "^5.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.509.0",
    "moment": "^2.30.1",
    "next": "~15.3.1",
    "next-auth": "^5.0.0-beta",
    "nodemailer": "^7.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sonner": "^2.0.3",
    "winston": "^3.17.0",
    "zod": "^3.24.4",
    "zod-validation-error": "^3.4.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.0",
    "@faker-js/faker": "^9.7.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.62",
    "@types/react-dom": "^19.0.0",
    "@types/winston": "^2.4.4",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.3.1",
    "prisma": "^6.7.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
